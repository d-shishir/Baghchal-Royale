{
  "expo": {
    "name": "Baghchal Royale",
    "slug": "baghchal-royale",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/br.png",
    "splash": {
      "image": "./assets/br.png",
      "backgroundColor": "#1a1a1a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.baghchal.royale",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/br.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.baghchal.royale",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-audio"
    ],
    "scheme": "baghchal-royale",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": false
    },
    "extra": {
      "eas": {
        "projectId": "b119503f-f0d4-4d14-b239-310e7b085a52"
      }
    }
  }
}
